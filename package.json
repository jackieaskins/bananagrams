{
  "name": "bananagrams",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc --project server/tsconfig.json && webpack --mode production",
    "check-types": "tsc --noEmit --project server/tsconfig.json && tsc --noEmit --project client/tsconfig.json",
    "dev": "nodemon",
    "lint": "eslint --ext .js,.ts,.tsx server/src client/src",
    "pre-commit": "npm run check-types && lint-staged && npm run lint && npm test && npm outdated",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:server": "jest --projects server",
    "test:server:watch": "jest --projects server --watch",
    "test:client": "jest --projects client",
    "test:client:watch": "jest --projects client --watch"
  },
  "engines": {
    "node": "18.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@types/enzyme": "^3.10.16",
    "@types/enzyme-adapter-react-16": "^1.0.9",
    "@types/express": "^4.17.21",
    "@types/node": "^14.18.63",
    "@types/react": "^16.14.51",
    "@types/react-dom": "^16.9.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-middleware": "^5.3.0",
    "@types/webpack-hot-middleware": "^2.25.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.3.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.14.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/jest": "^26.0.24",
    "express": "^4.18.2",
    "local-storage": "^2.0.0",
