    "socket.io-client": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "{server/client}/src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
