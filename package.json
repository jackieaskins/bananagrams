    "@types/jest": "^29.5.8",
    "@types/node": "^18.11.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^9.0.7",
