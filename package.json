{
  "name": "bananagrams",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc --project server/tsconfig.json && webpack --mode production",
    "check-types": "tsc --noEmit --project server/tsconfig.json && tsc --noEmit --project client/tsconfig.json",
    "dev": "nodemon",
    "lint": "eslint --ext .js,.ts,.tsx server/src client/src",
    "pre-commit": "npm outdated && npm run check-types && lint-staged && npm run lint && npm test",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:server": "jest --projects server",
    "test:server:watch": "jest --projects server --watch",
    "test:client": "jest --projects client",
    "test:client:watch": "jest --projects client --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.3",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.33",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/jest": "^26.0.10",
    "express": "^4.17.1",
    "local-storage": "^2.0.0",
    "notistack": "^0.9.17",
    "react": "^16.13.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
