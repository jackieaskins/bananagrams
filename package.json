    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.3.0"
  },
  "lint-staged": {
    "{server/client}/src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
